# -*- mode: snippet -*-
# name: prefix sum
# key: prefixsum
# --
struct PrefixSum {
  vector<long long int> sum;
  PrefixSum(vector<long long int> v) {
    sum.push_back(0);
    for (int i = 0; i < v.size(); ++i) {
      sum.push_back(sum.back() + v[i]);
    }
  }
  long long int operator () (size_t begin, size_t end) const {
    assert(begin <= end);
    return sum[end] - sum[begin];
  }
};
